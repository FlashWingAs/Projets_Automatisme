<Routines>
<Routine Name="Logic" Type="ST">
<STContent>
<Line Number="0">
<![CDATA[// ////////////////////////////////////////////////////////]]>
</Line>
<Line Number="1">
<![CDATA[// Defaut]]>
</Line>
<Line Number="2">
<![CDATA[// ////////////////////////////////////////////////////////]]>
</Line>
<Line Number="3">
<![CDATA[IF	(St_MC_Erreur OR RAZ_AOI) AND NOT St_RAZ THEN]]>
</Line>
<Line Number="4">
<![CDATA[		Graph:=0;]]>
</Line>
<Line Number="5">
<![CDATA[		St_RAZ:=1;]]>
</Line>
<Line Number="6">
<![CDATA[END_IF;]]>
</Line>
<Line Number="7">
<![CDATA[]]>
</Line>
<Line Number="8">
<![CDATA[// Acquit Defaut]]>
</Line>
<Line Number="9">
<![CDATA[IF Acquit AND St_RAZ THEN]]>
</Line>
<Line Number="10">
<![CDATA[	St_RAZ:=0;]]>
</Line>
<Line Number="11">
<![CDATA[	Defaut_MSO:=0;]]>
</Line>
<Line Number="12">
<![CDATA[	Motion_Control_MAS1.FLAGS	:= 0;]]>
</Line>
<Line Number="13">
<![CDATA[	Motion_Control_MAM.FLAGS	:= 0;]]>
</Line>
<Line Number="14">
<![CDATA[	Motion_Control_MAS2.FLAGS	:= 0;]]>
</Line>
<Line Number="15">
<![CDATA[END_IF;]]>
</Line>
<Line Number="16">
<![CDATA[]]>
</Line>
<Line Number="17">
<![CDATA[// TON Defaut MSO]]>
</Line>
<Line Number="18">
<![CDATA[TONR_Defaut_MSO.PRE				:= 5000;]]>
</Line>
<Line Number="19">
<![CDATA[TONR_Defaut_MSO.TimerEnable		:= Graph=20;]]>
</Line>
<Line Number="20">
<![CDATA[TONR_Defaut_MSO.Reset			:= NOT Graph=20;]]>
</Line>
<Line Number="21">
<![CDATA[TONR(TONR_Defaut_MSO);]]>
</Line>
<Line Number="22">
<![CDATA[]]>
</Line>
<Line Number="23">
<![CDATA[//Determination de la Position Unwind]]>
</Line>
<Line Number="24">
<![CDATA[IF Motion_Group.GroupSynced THEN ]]>
</Line>
<Line Number="25">
<![CDATA[	GSV(Axis,]]>
</Line>
<Line Number="26">
<![CDATA[		Axe,]]>
</Line>
<Line Number="27">
<![CDATA[		PositionUnwind,]]>
</Line>
<Line Number="28">
<![CDATA[		Axe_Position_Unwind_Temp);	]]>
</Line>
<Line Number="29">
<![CDATA[	GSV(Axis,]]>
</Line>
<Line Number="30">
<![CDATA[		Axe,]]>
</Line>
<Line Number="31">
<![CDATA[		ConversionConstant,]]>
</Line>
<Line Number="32">
<![CDATA[		Axe_Conversion_Constant);	]]>
</Line>
<Line Number="33">
<![CDATA[	Axe_Position_Unwind := Axe_Position_Unwind_Temp / Axe_Conversion_Constant;]]>
</Line>
<Line Number="34">
<![CDATA[ELSE]]>
</Line>
<Line Number="35">
<![CDATA[	Axe_Position_Unwind := 0;]]>
</Line>
<Line Number="36">
<![CDATA[END_IF;]]>
</Line>
<Line Number="37">
<![CDATA[]]>
</Line>
<Line Number="38">
<![CDATA[]]>
</Line>
<Line Number="39">
<![CDATA[// ////////////////////////////////////////////////////////]]>
</Line>
<Line Number="40">
<![CDATA[// Etat]]>
</Line>
<Line Number="41">
<![CDATA[// ////////////////////////////////////////////////////////]]>
</Line>
<Line Number="42">
<![CDATA[St_En_Attente:=0;]]>
</Line>
<Line Number="43">
<![CDATA[St_Init1:=0;]]>
</Line>
<Line Number="44">
<![CDATA[St_Init2:=0;]]>
</Line>
<Line Number="45">
<![CDATA[St_Activation:=0;]]>
</Line>
<Line Number="46">
<![CDATA[St_Pos_en_cours:=0;]]>
</Line>
<Line Number="47">
<![CDATA[St_Arret:=0;]]>
</Line>
<Line Number="48">
<![CDATA[St_Desactivation:=0;]]>
</Line>
<Line Number="49">
<![CDATA[]]>
</Line>
<Line Number="50">
<![CDATA[// ////////////////////////////////////////////////////////]]>
</Line>
<Line Number="51">
<![CDATA[// Cycle]]>
</Line>
<Line Number="52">
<![CDATA[// ////////////////////////////////////////////////////////]]>
</Line>
<Line Number="53">
<![CDATA[CASE Graph OF]]>
</Line>
<Line Number="54">
<![CDATA[	// Depart]]>
</Line>
<Line Number="55">
<![CDATA[	0:	]]>
</Line>
<Line Number="56">
<![CDATA[		IF NOT St_RAZ THEN]]>
</Line>
<Line Number="57">
<![CDATA[			St_En_Attente:=1;]]>
</Line>
<Line Number="58">
<![CDATA[		END_IF;]]>
</Line>
<Line Number="59">
<![CDATA[		]]>
</Line>
<Line Number="60">
<![CDATA[		IF Autorisation AND NOT Axe.StoppingStatus AND NOT RAZ_AOI AND NOT St_RAZ THEN]]>
</Line>
<Line Number="61">
<![CDATA[			Graph:=3;]]>
</Line>
<Line Number="62">
<![CDATA[		END_IF;]]>
</Line>
<Line Number="63">
<![CDATA[]]>
</Line>
<Line Number="64">
<![CDATA[	// Init des etats]]>
</Line>
<Line Number="65">
<![CDATA[	3:]]>
</Line>
<Line Number="66">
<![CDATA[		St_Init1:=1;]]>
</Line>
<Line Number="67">
<![CDATA[		// Init des etats]]>
</Line>
<Line Number="68">
<![CDATA[		Motion_Control_MAS1.FLAGS	:= 0;]]>
</Line>
<Line Number="69">
<![CDATA[		Motion_Control_MAM.FLAGS	:= 0;]]>
</Line>
<Line Number="70">
<![CDATA[		Motion_Control_MAS2.FLAGS	:= 0;]]>
</Line>
<Line Number="71">
<![CDATA[		// Acquit des erreurs]]>
</Line>
<Line Number="72">
<![CDATA[		St_RAZ:=0;]]>
</Line>
<Line Number="73">
<![CDATA[		Graph:=10;]]>
</Line>
<Line Number="74">
<![CDATA[]]>
</Line>
<Line Number="75">
<![CDATA[	// Arret si mouvement en cours]]>
</Line>
<Line Number="76">
<![CDATA[	10:]]>
</Line>
<Line Number="77">
<![CDATA[		St_Init2:=1;]]>
</Line>
<Line Number="78">
<![CDATA[		IF NOT Motion_Control_MAS1.EN THEN]]>
</Line>
<Line Number="79">
<![CDATA[			MAS(Axe,]]>
</Line>
<Line Number="80">
<![CDATA[					Motion_Control_MAS1, ]]>
</Line>
<Line Number="81">
<![CDATA[					All,]]>
</Line>
<Line Number="82">
<![CDATA[					Yes,]]>
</Line>
<Line Number="83">
<![CDATA[					Dec ,]]>
</Line>
<Line Number="84">
<![CDATA[					Unitspersec2,]]>
</Line>
<Line Number="85">
<![CDATA[					Yes,]]>
</Line>
<Line Number="86">
<![CDATA[					Jerk,]]>
</Line>
<Line Number="87">
<![CDATA[					%ofTime);]]>
</Line>
<Line Number="88">
<![CDATA[		END_IF;]]>
</Line>
<Line Number="89">
<![CDATA[		// Condition]]>
</Line>
<Line Number="90">
<![CDATA[		IF Motion_Control_MAS1.EN AND Motion_Control_MAS1.DN AND NOT Motion_Control_MAS1.IP AND NOT Axe.StoppingStatus THEN]]>
</Line>
<Line Number="91">
<![CDATA[			Graph:=20;]]>
</Line>
<Line Number="92">
<![CDATA[		END_IF;]]>
</Line>
<Line Number="93">
<![CDATA[]]>
</Line>
<Line Number="94">
<![CDATA[	// Activation]]>
</Line>
<Line Number="95">
